{
  "name": "slap",
  "version": "0.1.59",
  "description": "Sublime-like terminal-based text editor",
  "homepage": "https://github.com/slap-editor/slap#readme",
  "bugs": "https://github.com/slap-editor/slap/issues",
  "main": "./src/index.js",
  "bin": "./slap",
  "preferGlobal": true,
  "scripts": {
    "start": "./slap",
    "debug": "node-debug ./src/index.js",
    "test": "./test/index.js",
    "cover": "istanbul cover ./test/index.js",
    "build": "rm -rf ./dist && babel ./src --out-dir ./dist"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/slap-editor/slap.git"
  },
  "dependencies": {
    "base-widget": "1.0.10",
    "blessed": "0.1.81",
    "bluebird": "3.1.1",
    "editor-widget": "1.1.1",
    "lodash": "3.10.1",
    "mkdirp": "0.5.1",
    "node-clap": "0.0.5",
    "rc": "1.1.6",
    "react": "0.14.5",
    "react-blessed": "0.1.7",
    "slap-util": "1.0.6",
    "ttys": "0.0.3",
    "update-notifier": "0.6.0"
  },
  "devDependencies": {
    "babel-cli": "6.3.17",
    "babel-preset-es2015": "6.3.13",
    "babel-preset-react": "6.3.13",
    "get-random-port": "0.0.1",
    "istanbul": "0.4.1",
    "node-inspector": "0.12.5",
    "tape": "4.4.0"
  },
  "author": "Dan Kaplun <dbkaplun@gmail.com>",
  "license": "MIT"
}
